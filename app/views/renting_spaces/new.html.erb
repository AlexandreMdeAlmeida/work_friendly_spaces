<div class="container">
  <div class="col-md-6">
    <h1>Add a new Hive</h1>
    <%= simple_form_for @renting_space do |f| %>
      <%= f.input :name %>
      <%= f.input :address %>
      <%= f.input :amenities %>
      <%= f.input :internet, as: :boolean  %>
      <%= f.button :submit, class: "btn btn-primary"%>
    <% end %>
  </div>

</div>
